{
    "name": "devjs/eloquent-resources",
    "description": "Package for generic resource classes. Interfaces, Controllers, Validations, Entities, Repositories, Criterias etc",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislaw Mnizhek",
            "email": "xdefrag@devjs.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/database": "^5.4",
        "illuminate/http": "^5.4",
        "illuminate/container": "^5.4",
        "illuminate/console": "^5.4",
        "nikic/php-parser": "^4.0",
        "symfony/finder": "^3.0",
        "illuminate/validation": "^5.4"
    },
    "require-dev" : {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Devjs\\EloquentResources\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devjs\\EloquentResources\\Tests\\" : "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devjs\\EloquentResources\\EloquentResourcesServiceProvider"
            ]
        }
    }
}
